@model List<Student>
<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h2>Students list</h2>
		</div>
		<div class="col-md-6">
			<a asp-action="Create">Create Student</a>
		</div>
		<div>
			<table class="table table-striped  ">
				<thead>
					<tr>
						<td>Id</td>
						<td>Name</td>
						<td>Branch</td>
						<td>Roll no</td>
						<td>Edit</td>
						<td>Delete</td>
					</tr>
				</thead>
				<tbody>
					@foreach(var obj in Model)
					{
						<tr>
							<td>@obj.StudentId</td>
							<td>@obj.Name</td>
							<td>@obj.Branch</td>
							<td>@obj.RollNo</td>
							<td>
								<a asp-controller="Student" asp-action="Edit" asp-route-studentId="@obj.StudentId" class="btn btn-dark">
									Edit
								</a>
							</td>
							<td>
								<a asp-controller="Student" asp-action="Delete" asp-route-studentId="@obj.StudentId" class="btn btn-danger">
									Delete
								</a>
							</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>
</div>
